                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-md-6">
                        <label for="firstName" class="control-label">Contact No</label>
                        <div>
                            @*<input type="text" id="firstName" placeholder="Contact No" class="form-control" autofocus>*@
                            @Html.TextBoxFor(model => model.ContactNo, new { @placeholder = "Contact No", @class = "form-control", @required = "required" })
                        </div>
                    </div>
                    <div class="col-md-6">
                        <label for="firstName" class="control-label">Address</label>
                        <div>
                            @*<input type="textarea" id="firstName" placeholder="Address" class="form-control" autofocus>*@
                            @Html.TextAreaFor(model => model.Address, new { @placeholder = "Address", @class = "form-control", @required = "required" })
                        </div>
                    </div>
                </div>
            </div>

            <div class="form-group">
                <div class="row">
                    <div class="col-md-6">
                        <label for="firstName" class="control-label">Why do you want to work with Rohingya Crisis Foundation?</label>
                        @*<input type="textarea" id="firstName" placeholder="Address" class="form-control" autofocus>*@
                        @Html.TextAreaFor(model => model.WhyQ1, new { @class = "form-control" })
                    </div>
                    <div class="col-md-6">
                        <label for="firstName" class="control-label">In which project, do you want to work?</label>
                        @*<input type="textarea" id="firstName" placeholder="Address" class="form-control" autofocus>*@
                        @Html.TextAreaFor(model => model.InQ2, new { @class = "form-control" })
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-md-6">
                        <label for="firstName" class="control-label">What kind of skills/experiences do you have?</label>
                        @*<input type="textarea" id="firstName" placeholder="Address" class="form-control" autofocus>*@
                        @Html.TextAreaFor(model => model.WhatQ3, new { @class = "form-control" })
                    </div>
                    <div class="col-md-6">
                        <label for="firstName" class="control-label">Languages Spoken</label>
                        <div>
                            @*<input type="text" id="firstName" placeholder="Languages" class="form-control" autofocus>*@
                            @Html.TextBoxFor(model => model.Language, new { @placeholder = "Languages", @class = "form-control", @required = "required" })
                        </div>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-md-12">
                        <label for="firstName" class="control-label">References</label>
                        <div>
                            <p>It is our policy to take one reference. Please give names and addresses of referee who have known you for around 12 months and who know you well enough to comment on your suitability for voluntary work.</p>
                        </div>
                    </div>
                    @*<div class="col-md-6">

                        </div>*@
                </div>
            </div>

            <div class="form-group">
                <div class="row">
                    <div class="col-md-3">
                        <label for="firstName" class="control-label">Referee Name</label>
                        <div>
                            @*<input type="text" id="firstName" placeholder="Referee Name" class="form-control" autofocus>*@
                            @Html.TextBoxFor(model => model.RefereeName, new { @placeholder = "Name", @class = "form-control" })
                        </div>
                    </div>
                    <div class="col-md-3">
                        <label for="firstName" class="control-label">Contact No</label>
                        <div>
                            @*<input type="text" id="firstName" placeholder="Contact No" class="form-control" autofocus>*@
                            @Html.TextBoxFor(model => model.RefereeConNumber, new { @placeholder = "Contact No", @class = "form-control" })
                        </div>
                    </div>
                    <div class="col-md-3">
                        <label for="firstName" class="control-label">Email Address</label>
                        <div>
                            @*<input type="text" id="firstName" placeholder="Email Address" class="form-control" autofocus>*@
                            @Html.TextBoxFor(model => model.RefereeEmail, new { @placeholder = "Email", @class = "form-control" })
                        </div>
                    </div>
                    <div class="col-md-3">
                        <label for="firstName" class="control-label">Relation to you</label>
                        <div>
                            @*<input type="text" id="firstName" placeholder="Relation" class="form-control" autofocus>*@
                            @Html.TextBoxFor(model => model.RefereeRelation, new { @placeholder = "Relation", @class = "form-control" })
                        </div>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-md-12">
                        <label for="firstName" class="control-label">Terms & Conditions for RCF Membership</label>
                        <div>
                            <ul>
                                <li>•Member must abide by the constitution, rules and regulations of the RCF.</li>
                                <li>•Member must pay the membership fee of £20 once.</li>
                                <li>•Member must pay monthly contribution fee of £10 within first week of every month.</li>
                                <li>•Member must attend meetings and programs of the RCF to be held from time to time.</li>
                            </ul>
                        </div>
                    </div>
                    @*<div class="col-md-6">

                        </div>*@
                </div>
            </div>


            <div class="form-group">
                <div class="row">
                    <div class="col-md-12">
                        <label for="firstName" class="control-label">Declaration</label>
                        <div><p>I confirm that, to the best of my knowledge and belief, the information provided above is complete and correct. I understand that omitting and misrepresenting any relevant facts may subsequently affect my membership being refuse or terminated.</p></div>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-md-6">
                        <label for="firstName" class="control-label">Print Full Name</label>
                        <div>
                            @*<input type="text" id="firstName" placeholder="Referee Name" class="form-control" autofocus>*@
                            @Html.TextBoxFor(model => model.PrintName, new { @placeholder = "Print Name", @class = "form-control", @required = "required" })
                        </div>
                    </div>
                    <div class="col-md-6">
                        <label for="birthDate" class="control-label">Date:</label>
                        <div>
                            @*<input type="date" id="birthDate" class="form-control">*@
                            @Html.TextBoxFor(model => model.PrintDate, new { @id = "birthDate", @type = "date", @class = "form-control", @required = "required" })
                        </div>
                    </div>
                </div>
            </div>


            <div class="form-group">
                <div class="row">
                    <div class="col-md-4">
                        @*<button type="submit" class="btn btn-primary btn-block">Register</button>*@
                        <input id="Submit1" type="submit" class="btn btn-primary btn-block" value="Register" />
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    @*<div class="col-md-6">
                            <h3>Membership fee of UK currency £20.00</h3>
                        </div>*@
                    <div class="col-md-2">
                        <button type="submit" class="btn btn-primary btn-block" disabled>Payment</button>
                    </div>
                    <div class="col-md-2">
                        <button type="submit" class="btn btn-primary btn-block" disabled>Paypal</button>
                    </div>
                </div>
            </div>

        </form> <!-- /form -->
    </div> <!-- ./container -->
