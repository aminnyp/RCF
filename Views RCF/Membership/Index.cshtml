            <div class="form-group">
                <div class="row">
                    <div class="col-md-6">
                        <label for="firstName" class="control-label">First Name</label>
                        <div>
                            @*<input type="text" id="firstName" placeholder="Full Name" class="form-control" autofocus>*@
                            @Html.TextBoxFor(model => model.FirstName, new { @placeholder = "First Name", @class = "form-control", @required = "required" })
                            @*<span class="help-block">First Name, eg.: Avijit, Priti, Arghya</span>*@
                        </div>
                    </div>
                    <div class="col-md-6">
                        <label for="firstName" class="control-label">Last Name</label>
                        <div>
                            @*<input type="text" id="firstName" placeholder="Last Name" class="form-control" autofocus>*@
                            @Html.TextBoxFor(model => model.LastName, new { @placeholder = "Last Name", @class = "form-control", @required = "required" })
                            @*<span class="help-block">Last Name, eg.: Banerjee, Dey, Sarkar</span>*@
                        </div>
                    </div>

                </div>

            </div>
            <div class="form-group">
