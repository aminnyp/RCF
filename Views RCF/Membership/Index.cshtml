                    </div>
                    <div class="col-md-6">
                        <label for="email" class="control-label">Password</label>
                        <div>

                            @Html.TextBoxFor(model => model.Password, new { @placeholder = "Password", @type = "password", @class = "form-control", @required = "required" })

                        </div>
                    </div>
                </div>
            </div>
